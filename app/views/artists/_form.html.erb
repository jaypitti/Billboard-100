<%= form_for @artist do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br />

  <%= f.label :genre %>
  <%= f.text_field :genre %>
  <br />

  <%= f.label :image %>
  <%= f.text_field :image %>
  <br />

  <%= f.label :website %>
  <%= f.text_field :website %>
  <br />

  <%= f.label :bio %>
  <%= f.text_area :bio %>
  <br />

  <%= f.submit %>
<% end %>