<hr />
<%= form_for [@artist, @song] do |f| %>
  <%= f.label 'Title' %>
  <%= f.text_field :title, required: true, autofocus: true %>
  <%= f.label 'artwork' %>
  <%= f.text_field :artwork, required: true, autofocus: true %>
  <%= f.label 'duration' %>
  <%= f.text_field :duration, required: true, autofocus: true %>
  <%= f.submit%>
<% end %>
